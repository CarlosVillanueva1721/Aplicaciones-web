@model ControlInventario.Models.Supplier

@{
    ViewBag.Title = "Details";
}
<div class="masthead">
    <div class="masthead-content space">
        <div class="container">
            <div>
                <h4>Supplier</h4>
                <hr />
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.SupplierCode)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupplierCode)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.SupplierName)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.SupplierName)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Email)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Email)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Phone)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Phone)
                    </dd>

                    <dt>
                        @Html.DisplayNameFor(model => model.Address)
                    </dt>

                    <dd>
                        @Html.DisplayFor(model => model.Address)
                    </dd>

                </dl>
            </div>
        </div>
    </div>
</div>


<p>
    <a href="@Url.Action("Index", "Suppliers")" class="btn btn-danger btn-circle"> <i class="fas fa-long-arrow-alt-left"></i></a>
    <a href="@Url.Action("Edit", "Suppliers", new { id = Model.Id })" class="btn btn-warning btn-circle"> <i class="fas fa-edit"></i></a>
</p>
